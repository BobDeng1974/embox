	@Runlevel(2) include embox.net.raw_sock
	@Runlevel(2) include embox.net.neighbour(neighbour_amount=2)
	@Runlevel(2) include embox.net.netfilter(amount_rules=0)

	include embox.kernel.lthread.lthread
	include embox.kernel.thread.core(thread_pool_size=2,thread_stack_size=3000)
	include embox.kernel.thread.signal.siginfoq(pool_sz=1)

	/* tty requires */
	include embox.kernel.thread.mutex
	include embox.driver.tty.task_breaking_disable

	@Runlevel(2) include embox.cmd.shell(history_size=2)
	include embox.init.setup_tty_diag
	@Runlevel(3) include embox.init.start_script(shell_name="diag_shell")

	include embox.cmd.net.ifconfig
	include embox.cmd.net.route
	include embox.cmd.net.arp
	include embox.util.log
	include embox.util.hashtable
	include embox.kernel.critical

	//include embox.kernel.irq_static
	include embox.kernel.irq

	include embox.mem.pool_adapter

	include embox.cmd.play(wav_sdram=true,audio_buf_size=4000000,wav_storage=2)
	include embox.cmd.record_stm32(wav_sdram=true,audio_buf_size=4000000)

	include embox.arch.arm.libarch

	include embox.compat.libc.stdio.print(support_floating=1)
	include embox.compat.libc.math
	include embox.compat.libc.stdio.file_pool(file_quantity=0)

	include embox.mem.heap_bm
	include embox.mem.static_heap(heap_size=0x10000,section="")
	include embox.mem.static_heap2(heap_size=58000)
	include embox.mem.bitmask(page_size=64)

	include embox.fs.driver.initfs_dvfs
	include embox.fs.driver.devfs_dvfs
	include embox.fs.rootfs_dvfs(fstype="initfs")

	include embox.driver.char_dev_dvfs
	include embox.driver.serial.uart_dev_dvfs

	include embox.fs.dvfs.core
	include embox.compat.posix.fs.all_dvfs
	include embox.fs.syslib.perm_stub
	include embox.driver.block_common
	include embox.driver.block_dvfs

	include third_party.bsp.stmf7cube.core
	include third_party.bsp.stmf7cube.cmsis
	include third_party.bsp.stmf7cube.sdram
	include third_party.bsp.stmf7cube.stm32f7_discovery
	include third_party.bsp.stmf7cube.stm32f7_discovery_audio
	include third_party.bsp.stmf7cube.stm32f7_discovery_sd

	//include embox.driver.sd.stm32f7_sd

	/*include third_party.pjproject.streamutil*/
	/*include third_party.pjproject.pjsua*/
	//include third_party.pjproject.simpleua
